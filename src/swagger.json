    "components": {
        "schemas": {
            "AtendenteSemSenha": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid",
                        "description": "Identificador único do atendente"
                    },
                    "nome": {
                        "type": "string",
                        "description": "Nome completo do atendente"
                    },
                    "login": {
                        "type": "string",
                        "description": "Login utilizado para acesso ao sistema"
                    },
                    "ativo": {
                        "type": "boolean",
                        "description": "Indica se o atendente está ativo"
                    }
                },
                "required": [
                    "id",
                    "nome",
                    "login",
                    "ativo"
                ],
                "example": {
                    "id": "123e4567-e89b-12d3-a456-426614174000",
                    "nome": "Max Verstappen",
                    "login": "paiDaMclaren@gmail.com",
                    "ativo": true
                }
            },
            "AtendenteComSenha": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid",
                        "description": "Identificador único do atendente"
                    },
                    "nome": {
                        "type": "string",
                        "description": "Nome completo do atendente"
                    },
                    "login": {
                        "type": "string",
                        "description": "Login utilizado para acesso ao sistema"
                    },
                    "senha": {
                        "type": "string",
                        "format": "password",
                        "description": "Senha de acesso do atendente"
                    },
                    "ativo": {
                        "type": "boolean",
                        "description": "Indica se o atendente está ativo"
                    }
                },
                "required": [
                    "nome",
                    "login",
                    "senha"
                ],
                "example": {
                    "id": "123e4567-e89b-12d3-a456-426614174000",
                    "nome": "Max Verstappen",
                    "login": "paiDaMclaren@gmail.com",
                    "senha": "1",
                    "ativo": true
                }
            },
            "Paciente": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid",
                        "description": "Identificador único do paciente"
                    },
                    "nome": {
                        "type": "string",
                        "description": "Nome completo do paciente"
                    }
                },
                "required": [
                    "id",
                    "nome"
                ],
                "example": {
                    "id": "987e6543-e21b-12d3-a456-426614174999",
                    "nome": "Gabriel Bortoleto"
                }
            },
            "PacienteCreate": {
                "type": "object",
                "properties": {
                    "nome": {
                        "type": "string",
                        "description": "Nome completo do paciente"
                    }
                },
                "required": [
                    "nome"
                ],
                "example": {
                    "nome": "Gabriel Bortoleto"
                }
            },
            "PacienteUpdate": {
                "type": "object",
                "properties": {
                    "nome": {
                        "type": "string",
                        "description": "Nome atualizado do paciente"
                    }
                },
                "required": [
                    "nome"
                ],
                "example": {
                    "nome": "Gabriel Bortoleto"
                }
            },
            "Setor": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid",
                        "description": "Identificador único do setor"
                    },
                    "nomeSetor": {
                        "type": "string",
                        "description": "Nome do setor"
                    },
                    "isPrimeiroContato": {
                        "type": "boolean",
                        "description": "Indica se o setor é responsável pelo primeiro contato"
                    }
                },
                "required": [
                    "id",
                    "nomeSetor",
                    "isPrimeiroContato"
                ],
                "example": {
                    "id": "321e4567-e89b-12d3-a456-426614174111",
                    "nomeSetor": "Recepção",
                    "isPrimeiroContato": true
                }
            },
            "SetorCreate": {
                "type": "object",
                "properties": {
                    "nomeSetor": {
                        "type": "string",
                        "description": "Nome do setor"
                    },
                    "isPrimeiroContato": {
                        "type": "boolean",
                        "description": "Indica se o setor é responsável pelo primeiro contato"
                    }
                },
                "required": [
                    "nomeSetor",
                    "isPrimeiroContato"
                ],
                "example": {
                    "nomeSetor": "Recepção",
                    "isPrimeiroContato": true
                }
            },
            "SetorUpdate": {
                "type": "object",
                "properties": {
                    "nomeSetor": {
                        "type": "string",
                        "description": "Nome atualizado do setor"
                    },
                    "isPrimeiroContato": {
                        "type": "boolean",
                        "description": "Indica se o setor é responsável pelo primeiro contato"
                    }
                },
                "required": [
                    "nomeSetor",
                    "isPrimeiroContato"
                ],
                "example": {
                    "nomeSetor": "Atendimento Geral",
                    "isPrimeiroContato": false
                }
            },
            "Atendimento": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid",
                        "description": "Identificador único do atendimento"
                    },
                    "ticket": {
                        "type": "integer",
                        "description": "Número sequencial do atendimento gerado pelo setor"
                    },
                    "prioridade": {
                        "type": "boolean",
                        "description": "Indica se o atendimento é prioritário"
                    },
                    "atendimentoInfantil": {
                        "type": "boolean",
                        "description": "Indica se o atendimento é infantil"
                    },
                    "status": {
                        "type": "string",
                        "description": "Status atual do atendimento",
                        "enum": [
